<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="jp.co.stnet.cms.example.application.repository.job01.EmployeeRepository">
    <select id="findAll" resultType="jp.co.stnet.cms.example.domain.model.job01.Employee">
        <![CDATA[
        SELECT
            empId, empName
        FROM
            employee
        ORDER BY
            empId ASC
        ]]>
    </select>
</mapper>